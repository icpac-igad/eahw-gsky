[{
        "id": "gsky-reload-config",
        "execute-command": "./scripts/gsky_reload_config.sh",
        "response-message": "Executing Reload Script",
        "http-methods": [
            "POST"
        ]
    },
    {
        "id": "gsky-ingest-data",
        "execute-command": "./scripts/gsky_ingest_data.sh",
        "response-message": "Executing Ingest Script",
        "http-methods": [
            "POST"
        ],
        "incoming-payload-content-type": "application/json",
        "trigger-rule": {
            "match": {
                "type": "payload-hmac-sha256",
                "secret": "",
                "parameter": {
                    "source": "header",
                    "name": "X-Gsky-Signature"
                }
            }
        }
    }
]